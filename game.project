[project]
title = DefoldKaiOS
version = 1.0
bundle_resources = bundle

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 240
height = 320
update_frequency = 30
vsync = 0

[bootstrap]
main_collection = /example/example.collectionc

[library]
include_dirs = kaios

[script]
shared_state = 1

[html5]
htmlfile = /kaios/engine_template.html

[physics]
gravity_y = 0.0
scale = 0.2

